<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.shinytomato.convox.pages.LanguageInspectionController"
      stylesheets="@../root.css, @languageInspection.css"
      prefHeight="500.0" prefWidth="600.0" spacing="25.0"
      alignment="TOP_CENTER">

    <fx:include fx:id="wordListView" source="../searchableList/searchableList.fxml"/>

    <VBox prefWidth="300" alignment="TOP_CENTER">
        <HBox alignment="CENTER_RIGHT">
            <Button fx:id="editorialModeButton">
                <graphic>
                    <ImageView fx:id="editorialModeButtonImage" fitHeight="16" fitWidth="16"/>
                </graphic>
            </Button>
        </HBox>
    </VBox>
</HBox>
